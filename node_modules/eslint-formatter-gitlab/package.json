    "c8": "^8.0.0",
    "codeclimate-types": "^0.3.0",
    "eslint": "^8.0.0",
    "eslint-config-remcohaszing": "^10.0.0",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-remcohaszing": "^2.0.0",
    "typescript": "^5.0.0"
